import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int []arr = new int[n];
		for(int i=0;i<n;i++){
		    arr[i] = sc.nextInt();
		}
		int index = 0;
		for(int i=n-1;i>=0;i--){
		    if(arr[i] % 2 != 0){
		        index = i;
		        break;
		    }
		}
		for(int i=index;i>=0;i--){
		    System.out.print(arr[i]);
		}
	}
}
